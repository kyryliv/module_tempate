/**
 * Implements hook_help().
 */
function funds_test_help($route_name) {
    $output = '';
    switch ($route_name) {
        // Main module help for the fund module.
        case 'help.page.fund':
            $output .= '<h3>' . t('About') . '</h3>';
            $output .= '<p>' . t('A simple example on how to interact with the Charts API') . '</p>';
            break;
    }
    return $output;
}

